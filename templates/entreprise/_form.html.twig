{{ form_start(form) }}
<div class="row">
	<div class="col">{{ form_row(form.designation)}}</div>
	<div class="col">{{ form_row(form.adress)}}</div>
	<div class="col">{{ form_row(form.contact)}}</div>
</div>
<div class="row">
	<div class="col">{{ form_row(form.email) }}</div>
	<div class="col">{{ form_row(form.latitude) }}</div>
	<div class="col">{{ form_row(form.longitude) }}</div>
	<div class="col">{{ form_row(form.partener) }}</div>
</div>
{{ form_widget(form) }}

<button class="btn btn-primary mb-2">{{ button_label|default('Save') }}</button>
{{ form_end(form) }}
